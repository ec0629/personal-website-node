{{> navBar league}}
<div id="leagueKey" data-value="{{league.leagueKey}}"></div>
<div class="container">
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            {{#each tableMetaData}}

            <th scope="col" class="{{sortClass}}" data-col-val="{{id}}">{{headerName}}
              {{{sortAsc}}}{{{sortDesc}}}</th>

            {{/each}}
          </tr>
        </thead>
        <tbody>
          {{#each playerData}}
          {{> tableRow}}
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
<script src="/js/research.js"></script>